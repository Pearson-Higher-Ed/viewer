{
  "name": "@pearson-incubator/viewer",
  "version": "0.1.37",
  "description": "Viewer component to render pages of a book",
  "main": "./main.js",
  "scripts": {
    "dev-intl": "npm run dev-setup && npm run pseudoloc && webpack-dev-server --port 8081 --devtool source-map --hot --progress --colors",
    "dev-setup": "mkdir -p fonts && cp ./node_modules/pearson-elements/dist/fonts/* ./fonts && cp ./node_modules/pearson-elements/dist/css/elements.css ./demo",
    "dev": "npm run dev-setup && webpack-dev-server --port 8081 --devtool source-map --hot --progress --colors",
    "pretest": "npm run lint",
    "test": "mocha --compilers js:babel-register --require babel-polyfill --require intl --require test/utils/dom.js",
    "build": "webpack -p",
    "lint": "eslint src/js/** main.js demo/demo.js",
    "gen-changelog": "node ./npm_scripts/gen-changelog.js",
    "version": "npm run gen-changelog && git add CHANGELOG.md",
    "ci": "npm test && npm run build",
    "release": "node ./npm_scripts/release.js",
    "verify": "node ./npm_scripts/verify.js",
    "prebuild": "npm run pseudoloc",
    "pseudoloc": "./node_modules/pseudoloc/bin/pseudoloc --readFile ./translations/en.json --writeFile ./translations/pseudo.json"
  },
  "author": "Arish Krishnan",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Pearson-Higher-Ed/"
  },
  "dependencies": {
    "@pearson-incubator/aquila-js-core": "^0.2.32",
    "contentful": "^3.5.0",
    "estraverse-fb": "^1.3.1",
    "lodash": "^4.16.4",
    "react-intl": "^2.3.0"
  },
  "devDependencies": {
    "@pearson-components/npm-scripts": "^0.1.0",
    "babel-core": "~6.3.17",
    "babel-eslint": "~4.1.6",
    "babel-loader": "~6.2.0",
    "babel-polyfill": "~6.7.4",
    "babel-preset-es2015": "~6.3.13",
    "babel-preset-react": "~6.3.13",
    "babel-preset-stage-0": "~6.5.0",
    "babel-register": "~6.7.2",
    "colors": "~1.1.2",
    "conventional-changelog": "~1.1.0",
    "css-loader": "~0.23.0",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.0.0",
    "eslint-loader": "~1.1.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^5.0.1",
    "eslint-plugin-promise": "^3.3.0",
    "eslint-plugin-react": "^7.0.1",
    "expect": "~1.18.0",
    "expect-jsx": "~2.5.1",
    "file-loader": "~0.8.5",
    "intl": "~1.1.0",
    "jsdom": "~8.5.0",
    "json-loader": "~0.5.4",
    "material-ui": "^0.16.1",
    "mocha": "~2.4.5",
    "node-sass": "^4.5.2",
    "pearson-elements": "^0.11.0",
    "prop-types": "^15.5.10",
    "pseudoloc": "^1.1.0",
    "react": "^15.4.2",
    "react-addons-test-utils": "^15.0.0",
    "react-dom": "^15.4.2",
    "sass-loader": "^4.1.1",
    "semver": "~5.1.0",
    "style-loader": "~0.13.0",
    "url-loader": "~0.5.7",
    "webpack": "~1.12.9",
    "webpack-dev-server": "~1.14.0"
  },
  "peerDependencies": {
    "material-ui": "^0.16.0",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-tap-event-plugin": "~2.0.1",
    "react-intl": "~2.3.0",
    "prop-types": "^15.5.10"
  },
  "keywords": [
    "pearson-components"
  ]
}
